{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "include": [
    "src/**/*.ts",
    "test/core/**/*.ts",
  ],
  "exclude": [
    "sequelize.config.js",
  ]
}